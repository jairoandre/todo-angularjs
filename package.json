{
  "name": "todo-angularjs",
  "version": "1.0.0",
  "description": "TODO list made with angularjs",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=production && node server/runner.js",
    "linux": "NODE_ENV=production node server/runner.js",
    "build": "webpack --progress --color",
    "prod": "./node_modules/.bin/npm-run-all build start",
    "dev-server": "node server/webpack",
    "dev-runner": "nodemon server/runner",
    "dev": "./node_modules/.bin/npm-run-all --parallel dev-server dev-runner",
    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.6",
    "angular-animate": "^1.5.6",
    "angular-aria": "^1.5.6",
    "angular-material": "^1.1.0-rc.5",
    "express": "^4.13.4",
    "http": "0.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.1.2",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
